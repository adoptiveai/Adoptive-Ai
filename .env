BROWSER=none
GENERATE_SOURCEMAP=false
NODE_OPTIONS=--no-deprecation

# Stop WebSocket and data spam
REACT_APP_LOG_WEBSOCKET_MESSAGES=false
REACT_APP_LOG_WEBSOCKET_CONNECTIONS=false
REACT_APP_LOG_API_REQUESTS=false
REACT_APP_LOG_API_RESPONSES=false
REACT_APP_LOG_DEBUG=false

# Keep important logs
REACT_APP_LOG_ERRORS=true
REACT_APP_LOG_TRADING_OPERATIONS=true